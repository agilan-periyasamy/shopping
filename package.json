{
  "name": "express-api-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate for writing pretty Express-based APIs",
  "scripts": {
    "dev-server": "nodemon --exec \"babel-node --inspect src\"",
    "build": "babel -d ./build src",
    "start": "npm run-script build && nodemon --exec \"NODE_ENV=development node build/index.js\"",
    "test": "jest",
    "deploy": "pm2 deploy ecosystem.config.js production",
    "lint": "eslint 'src/**/*.js'",
    "fix-lint": "prettier --write './src/**/*.js' && npm run-script lint"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osdiab/gymi-website-node.git"
  },
  "keywords": [
    "frontend",
    "boilerplate"
  ],
  "author": "Omar Diab (github/osdiab) and Rona Wo (github/YanrongWo)",
  "bugs": {
    "url": "https://github.com/osdiab/gymi-website-node/issues"
  },
  "homepage": "https://github.com/osdiab/gymi-website-node#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.1",
    "express-force-ssl": "^0.3.2",
    "express-request-language": "^1.1.12",
    "http-status-codes": "^1.4.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.0.1",
    "mysql2": "^1.7.0",
    "node-fetch": "^2.6.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.11",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "pm2": "^2.7.2",
    "prettier": "^1.7.3",
    "sequelize-cli": "^5.5.1"
  }
}
